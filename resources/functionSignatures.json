	"Transcode.Atrs_TrialwiseTrace": {
		"inputs": [
			{
				"name": "Atrs",
				"kind": "required",
				"type": [
					"column",
					"cell"
				],
				"purpose": "每个元胞内是一个Atr数据单元"
			},
			{
				"name": "TagCodes",
				"kind": "required",
				"type": [
					"size=:,2",
					"positive"
				],
				"purpose": "根据所需实验条件设置的Atr元胞索引"
			},
			{
				"name": "TimeIndices",
				"kind": "required",
				"type": [
					"row",
					"positive"
				],
				"purpose": "要取出的时间索引（采样帧，不是真实时间）"
			},
			{
				"name": "TrialsFromStart",
				"kind": "namevalue",
				"type": [
					"row",
					"positive"
				],
				"purpose": "从开头计算，要取出哪些Trial"
			},
			{
				"name": "TrialsFromEnd",
				"kind": "namevalue",
				"type": [
					"row",
					"positive"
				],
				"purpose": "从末尾倒数，要取出哪些Trial"
			}
		],
		"outputs": [
			{
				"name": "MeanLines",
				"type": [
					"2d",
					"double"
				]
			},
			{
				"name": "ErrorShadows",
				"type": [
					"2d",
					"double"
				]
			}
		]
	},
	"Transcode.MECgRaws_OverallHeatmap":{
		"inputs":[
			{
				"name":"MECgRaws",
				"kind":"required",
				"type":["3d","cell"],
				"purpose":"MECgRaws格式数据，详见自述文档"
			},
			{
				"name":"BaseLength",
				"kind":"required",
				"type":"positive",
				"purpose":"刺激前基线时间点数"
			},
			{
				"name":"Flags",
				"kind":"flag",
				"type":"choices={'DoLog','PickMedian'}",
				"repeating":true,
				"purpose":"树立旗帜，是否启用特殊功能"
			}
		],
		"outputs":[
			{
				"name":"OHData",
				"type":["3d","double"]
			}
		]
	},
	"Transcode.MECgSplitTrialsToExperimentsByTag": {
		"inputs": [
			{
				"name": "CgRaw",
				"kind": "required",
				"type": [
					"size=1,1,:",
					"cell"
				],
				"purpose": "MECgRaws源数据，Mouse和Experiment维度单一，可以有多个CellGroup"
			},
			{
				"name": "TagTable",
				"kind": "required",
				"type": "tabular",
				"purpose": "METags元胞内的单个表格，与CgRaw的Mouse和Experiment对应"
			},
			{
				"name": "ClassifyFunction",
				"kind": "required",
				"type": "function_handle",
				"purpose": "分标函数句柄。接受TagTable的某一行作为输入，输出分Experiment标识用于分组"
			}
		],
		"outputs": [
			{
				"name": "RawSplit",
				"type": [
					"size=:,1,:",
					"cell"
				]
			},
			{
				"name": "TagSplit",
				"type": [
					"column",
					"cell"
				]
			}
		]
	},
	"Transcode.MECgSplitTrialsToExperimentsEqually": {
		"inputs": [
			{
				"name": "CgRaw",
				"kind": "required",
				"type": [
					"size=1,1,:",
					"cell"
				],
				"purpose": "MECgRaws源数据，Mouse和Experiment维度单一，可以有多个CellGroup"
			},
			{
				"name": "TagTable",
				"kind": "required",
				"type": "tabular",
				"purpose": "METags元胞内的单个表格，与CgRaw的Mouse和Experiment对应"
			},
			{
				"name": "SplitNumber",
				"kind": "required",
				"type": "positive",
				"purpose": "拆分个数"
			}
		],
		"outputs": [
			{
				"name": "RawSplit",
				"type": [
					"size=:,1,:",
					"cell"
				]
			},
			{
				"name": "TagSplit",
				"type": [
					"column",
					"cell"
				]
			}
		]
	},
	"Transcode.SortBeforeOH": {
		"inputs": [
			{
				"name": "Data",
				"kind": "required",
				"type": [
					"3d",
					"numeric"
				],
				"purpose": "第1维是泳道内不同的行，第2维是泳道内不同的列，第3维是不同的泳道"
			}
		],
		"outputs": [
			{
				"name": "Data",
				"type": [
					"3d",
					"numeric"
				]
			}
		]
	},
	"DrawFigure.ElasticChart": {
		"inputs": [
			{
				"name": "DistanceMatrix",
				"kind": "required",
				"type": [
					"square",
					"numeric"
				],
				"purpose": "两两点间距离的精确值。无数据的请填入NaN"
			},
			{
				"name": "PointNames",
				"kind": "required",
				"type": [
					"string",
					"vector"
				],
				"purpose": "每个点的名称"
			},
			{
				"name": "Iteration",
				"kind": "required",
				"type": "positive",
				"purpose": "迭代次数。次数越多越精细准确，但耗时越长"
			},
			{
				"name": "Positions",
				"kind": "ordered",
				"type": [
					"vector",
					"complex"
				],
				"purpose": "每个点的初始位置，行向量，用复数表示"
			},
			{
				"name": "ScatterStyle",
				"kind": "namevalue",
				"type": [
					"vector",
					"cell"
				],
				"purpose": "散点样式，将传递给scatter",
				"default": "{[],'green'}"
			},
			{
				"name": "TextStyle",
				"kind": "namevalue",
				"type": [
					"vector",
					"cell"
				],
				"purpose": "文字样式，将传递给text",
				"default": "{'Color','red','HorizontalAlignment','center','VerticalAlignment','middle'}"
			},
			{
				"name": "PlotStyle",
				"kind": "namevalue",
				"type": [
					"vector",
					"cell"
				],
				"purpose": "线条样式，将传递给plot",
				"default": "{'blue'}"
			}
		],
		"outputs": [
			{
				"name": "Positions",
				"type": [
					"row",
					"complex"
				]
			}
		]
	},
	"DrawFigure.OverallHeatmap": {
		"inputs": [
			{
				"name": "Data",
				"kind": "required",
				"type": [
					"3d",
					"numeric"
				],
				"purpose": "作图数据"
			},
			{
				"name": "Flags",
				"kind": "ordered",
				"type": "choices={'HideXAxis','HideYAxis','ShowColorBar'}",
				"repeating": true,
				"purpose": "重复设置功能旗帜"
			},
			{
				"name": "Colormap",
				"kind": "ordered",
				"type": [
					"double",
					"size=:,3"
				],
				"purpose": "颜色映射"
			},
			{
				"name": "ImagescStyle",
				"kind": "namevalue",
				"type": [
					"row",
					"cell"
				],
				"purpose": "要传递给imagesc的其它参数"
			},
			{
				"name": "SubTitles",
				"kind": "namevalue",
				"type": [
					"row",
					"string"
				],
				"purpose": "每个泳道的小标题"
			},
			{
				"name": "CBLabel",
				"kind": "namevalue",
				"type": [
					"scalar",
					"string"
				],
				"purpose": "颜色棒的标签"
			},
			{
				"name": "TLStyle",
				"kind": "namevalue",
				"type": [
					"row",
					"cell"
				],
				"purpose": "要传递给tiledlayout的其它参数",
				"default": "{'TileSpacing','none','Padding','compact'}"
			}
		],
		"outputs": [
			{
				"name": "Layout",
				"type": [
					"scalar",
					"matlab.graphics.layout.TiledChartLayout"
				]
			},
			{
				"name": "ColorBar",
				"type": [
					"scalar",
					"matlab.graphics.illustration.ColorBar"
				]
			}
		]
	},
	"DrawFigure.TrialwiseTrace": {
		"inputs": [
			{
				"name": "MeanLines",
				"kind": "required",
				"type": [
					"2d",
					"numeric"
				],
				"purpose": "每一行是一条平均线上的不同时点数值，不同行是不同的平均线"
			},
			{
				"name": "ErrorShadows",
				"kind": "required",
				"type": [
					"2d",
					"numeric"
				],
				"purpose": "每一行是一条平均线不同时点的误差，不同行是不同线的误差"
			},
			{
				"name": "XSpacing",
				"kind": "positional",
				"type": [
					"scalar",
					"numeric",
					">=0"
				],
				"purpose": "每条线之间的X距离",
				"default": 1
			},
			{
				"name": "StimuRange",
				"kind": "namevalue",
				"type": [
					"size=1,2",
					"numeric"
				],
				"purpose": "刺激所在的时间范围"
			},
			{
				"name": "Xs",
				"kind": "namevalue",
				"type": [
					"row",
					"numeric"
				],
				"purpose": "各时点的X值"
			},
			{
				"name": "YLimit",
				"kind": "namevalue",
				"type": [
					"size=1,2",
					"numeric"
				],
				"purpose": "Y轴数值范围"
			},
			{
				"name": "LineStyle",
				"kind": "namevalue",
				"type": [
					"row",
					"cell"
				],
				"purpose": "均值折线的样式，将传递给plot函数实现",
				"default": "{'b'}"
			},
			{
				"name": "ShadowStyle",
				"kind": "namevalue",
				"type": [
					"row",
					"cell"
				],
				"purpose": "误差阴影的样式，将传递给fill函数实现",
				"default": "{'b','FaceAlpha',0.2,'LineStyle','none'}"
			},
			{
				"name": "StimuStyle",
				"kind": "namevalue",
				"type": [
					"row",
					"cell"
				],
				"purpose": "刺激范围的样式，将传递给fill函数实现",
				"default": "{'r','FaceAlpha',0.2,'LineStyle','none'}"
			}
		],
		"outputs": [
			{
				"name": "Lines",
				"type": [
					"column",
					"matlab.graphics.chart.primitive.Line"
				]
			},
			{
				"name": "Shadows",
				"type": [
					"column",
					"matlab.graphics.primitive.Patch"
				]
			},
			{
				"name": "Stimuli",
				"type": [
					"column",
					"matlab.graphics.primitive.Patch"
				]
			}
		]
	},
	"LMHColorMap": {
		"inputs": [
			{
				"name": "Lowest",
				"kind": "required",
				"type": "size=1,3",
				"purpose": "最小值的RGB颜色，应在[0,1]之间"
			},
			{
				"name": "Middle",
				"kind": "required",
				"type": "size=1,3",
				"purpose": "中间值的RGB颜色，应在[0,1]之间"
			},
			{
				"name": "Highest",
				"kind": "required",
				"type": "size=1,3",
				"purpose": "最大值的RGB颜色，应在[0,1]之间"
			},
			{
				"name": "LowSteps",
				"kind": "ordered",
				"type": "positive",
				"purpose": "低段区分色阶数",
				"default": 255
			},
			{
				"name": "HighSteps",
				"kind": "ordered",
				"type": "positive",
				"purpose": "高段区分色阶数",
				"default": 255
			}
		],
		"outputs": [
			{
				"name": "Colormap",
				"type": [
					"size=:,3",
					"double"
				]
			}
		]
	},
	"SplitOneSingleDimensionByAnother": {
		"inputs": [
			{
				"name": "Array",
				"kind": "required",
				"purpose": "多维数组"
			},
			{
				"name": "SingleDimension",
				"kind": "required",
				"type": "positive",
				"purpose": "要扩张的单一维度"
			},
			{
				"name": "GroupDimension",
				"kind": "required",
				"type": "positive",
				"purpose": "要拆分的维度"
			},
			{
				"name": "GroupMarkers",
				"kind": "required",
				"type": "vector",
				"purpose": "分组标记向量，其长度必须等于Array的GroupDimension维度的长度"
			},
			{
				"name": "EmptyValue",
				"kind": "required",
				"type": "scalar",
				"purpose": "填补空位的标量值"
			}
		],
		"outputs": [
			{
				"name": "SplitArray"
			},
			{
				"name": "UniqueMarkers",
				"type": "column"
			}
		]
	}
}