{
	"DrawFigure.ShadowedLine": {
		"inputs": [
			{
				"name": "LineYs",
				"kind": "required",
				"type": [
					"row",
					"numeric"
				],
				"purpose": "平均值折线Y值，将用plot函数作出"
			},
			{
				"name": "ShadowHeights",
				"kind": "required",
				"type": [
					"row",
					"numeric"
				],
				"purpose": "误差范围阴影高度，将用fill函数作出"
			},
			{
				"name": "Xs",
				"kind": "ordered",
				"type": [
					"row",
					"numeric"
				],
				"purpose": "X轴对应数值向量"
			},
			{
				"name": "LineStyle",
				"kind": "namevalue",
				"type": [
					"row",
					"cell"
				],
				"purpose": "均值折线的样式，将传递给plot函数实现"
			},
			{
				"name": "ShadowStyle",
				"kind": "namevalue",
				"type": [
					"row",
					"cell"
				],
				"purpose": "误差阴影的样式，将传递给fill函数实现"
			}
		],
		"outputs": [
			{
				"name": "Line",
				"type": [
					"scalar",
					"matlab.graphics.chart.primitive.Line"
				]
			},
			{
				"name": "Shadow",
				"type": [
					"scalar",
					"matlab.graphics.primitive.Patch"
				]
			}
		]
	},
	"DrawFigure.OverallHeatmap": {
		"inputs": [
			{
				"name": "Data",
				"kind": "required",
				"type": [
					"3d",
					"numeric"
				],
				"purpose": "作图数据"
			},
			{
				"name": "Flag",
				"kind": "flag",
				"type": "choices={'HideXAxis','HideYAxis','ShowColorbar'}",
				"repeating": true,
				"purpose": "是否隐藏坐标轴、显示颜色棒"
			},
			{
				"name": "Colormap",
				"kind": "namevalue",
				"type": [
					"double",
					"size=:,3"
				],
				"purpose": "颜色映射"
			},
			{
				"name": "ImagescStyle",
				"kind": "namevalue",
				"type": [
					"row",
					"cell"
				],
				"purpose": "要传递给imagesc的其它参数"
			},
			{
				"name": "SubTitles",
				"kind": "namevalue",
				"type": [
					"row",
					"string"
				],
				"purpose": "每个泳道的小标题"
			},
			{
				"name": "CBLabel",
				"kind": "namevalue",
				"type": [
					"scalar",
					"string"
				],
				"purpose": "颜色棒的标签"
			},
			{
				"name": "TLStyle",
				"kind": "namevalue",
				"type": [
					"row",
					"cell"
				],
				"purpose": "要传递给tiledlayout的其它参数",
				"default": "{'TileSpacing','none','Padding','compact'}"
			}
		],
		"outputs": [
			{
				"name": "Layout",
				"type": [
					"scalar",
					"matlab.graphics.layout.TiledChartLayout"
				]
			}
		]
	},
	"DrawFigure.TrialwiseTrace": {
		"inputs": [
			{
				"name": "MeanLines",
				"kind": "required",
				"type": [
					"2d",
					"numeric"
				],
				"purpose": "每一行是一条平均线上的不同时点数值，不同行是不同的平均线"
			},
			{
				"name": "ErrorShadows",
				"kind": "required",
				"type": [
					"2d",
					"numeric"
				],
				"purpose": "每一行是一条平均线不同时点的误差，不同行是不同线的误差"
			},
			{
				"name": "XSpacing",
				"kind": "positional",
				"type": [
					"scalar",
					"numeric",
					">=0"
				],
				"purpose": "每条线之间的X距离",
				"default": 1
			},
			{
				"name": "StimuRange",
				"kind": "namevalue",
				"type": [
					"size=1,2",
					"numeric"
				],
				"purpose": "刺激所在的时间范围"
			},
			{
				"name": "Xs",
				"kind": "namevalue",
				"type": [
					"row",
					"numeric"
				],
				"purpose": "各时点的X值"
			},
			{
				"name": "YLimit",
				"kind": "namevalue",
				"type": [
					"size=1,2",
					"numeric"
				],
				"purpose": "Y轴数值范围"
			},
			{
				"name": "LineStyle",
				"kind": "namevalue",
				"type": [
					"row",
					"cell"
				],
				"purpose": "均值折线的样式，将传递给plot函数实现",
				"default": "{'b'}"
			},
			{
				"name": "ShadowStyle",
				"kind": "namevalue",
				"type": [
					"row",
					"cell"
				],
				"purpose": "误差阴影的样式，将传递给fill函数实现",
				"default": "{'b','FaceAlpha',0.2,'LineStyle','none'}"
			},
			{
				"name": "StimuStyle",
				"kind": "namevalue",
				"type": [
					"row",
					"cell"
				],
				"purpose": "刺激范围的样式，将传递给fill函数实现",
				"default": "{'r','FaceAlpha',0.2,'LineStyle','none'}"
			}
		],
		"outputs": [
			{
				"name": "Lines",
				"type": [
					"column",
					"matlab.graphics.chart.primitive.Line"
				]
			},
			{
				"name": "Shadows",
				"type": [
					"column",
					"matlab.graphics.primitive.Patch"
				]
			},
			{
				"name": "Stimuli",
				"type": [
					"column",
					"matlab.graphics.primitive.Patch"
				]
			}
		]
	},
	"CollectData.Rdc3_Atr": {
		"inputs": [
			{
				"name": "FilePath",
				"kind": "required",
				"type": "file=*.mat",
				"purpose": "Rdc2格式文件"
			}
		],
		"outputs": [
			{
				"name": "atr",
				"type": [
					"2d",
					"cell"
				]
			}
		]
	},
	"Transcode.Atrs_TrialwiseTrace": {
		"inputs": [
			{
				"name": "Atrs",
				"kind": "required",
				"type": [
					"column",
					"cell"
				],
				"purpose": "每个元胞内是一个Atr数据单元"
			},
			{
				"name": "TagCodes",
				"kind": "required",
				"type": [
					"size=:,2",
					"positive"
				],
				"purpose": "根据所需实验条件设置的Atr元胞索引"
			},
			{
				"name": "TimeIndices",
				"kind": "required",
				"type": [
					"row",
					"positive"
				],
				"purpose": "要取出的时间索引（采样帧，不是真实时间）"
			},
			{
				"name": "TrialsFromStart",
				"kind": "namevalue",
				"type": [
					"row",
					"positive"
				],
				"purpose": "从开头计算，要取出哪些Trial"
			},
			{
				"name": "TrialsFromEnd",
				"kind": "namevalue",
				"type": [
					"row",
					"positive"
				],
				"purpose": "从末尾倒数，要取出哪些Trial"
			}
		],
		"outputs": [
			{
				"name": "MeanLines",
				"type": [
					"2d",
					"double"
				]
			},
			{
				"name": "ErrorShadows",
				"type": [
					"2d",
					"double"
				]
			}
		]
	}
}