{
	"CollectData.MetaTags_METags": {
		"inputs": [
			{
				"name": "MTPaths",
				"kind": "required",
				"type": [
					"vector",
					"string"
				],
				"purpose": "MetaTags文件路径"
			},
			{
				"name": "SizeT",
				"kind": "required",
				"type": "positive",
				"purpose": "每个文件要截取的时间帧数，应当短于所有文件中的时间帧数"
			},
			{
				"name": "TagThreshold",
				"kind": "ordered",
				"type": "positive",
				"purpose": "将Tag值逻辑化时的阈值",
				"default": 550
			}
		],
		"outputs": [
			{
				"name": "Mice",
				"type": [
					"column",
					"string"
				]
			},
			{
				"name": "Experiments",
				"type": [
					"column",
					"string"
				]
			},
			{
				"name": "Tags",
				"type": [
					"2d",
					"cell"
				]
			}
		]
	},
	"CollectData.MTRM_Rdc3":{
		"inputs":[
			{
				"name":"TagThreshold",
				"kind":"ordered",
				"type":"positive",
				"purpose":"分标阈值",
